		"fabric-shim": "^2.2.0"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"chai-as-promised": "^7.1.1",
		"eslint": "^6.8.0",
		"mocha": "^7.1.1",
		"nyc": "^15.0.0",
		"sinon": "^9.0.1",
		"sinon-chai": "^3.5.0",
		"winston": "^3.2.1",
		"@imaginary-cloud/eslint-config-react": "^1.0.1",
		"@imaginary-cloud/prettier-config": "^1.0.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-config-prettier": "^6.15.0",
